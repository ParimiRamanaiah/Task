{
  "authenticationMethod": "route",
  "sessionTimeout": 10,
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/appconfig/",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "srv-api",
      "csrfProtection": false
    }
  ],
  "logout": {
    "logoutEndpoint": "/my/logout",
    "logoutPage": "https://www.google.com"
  },
  "welcomeFile": "nsscreens"
}
